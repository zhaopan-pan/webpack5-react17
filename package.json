{
    "name": "webpack-flow",
    "version": "1.0.0",
    "description": "a react project template",
    "private": true,
    "author": "zp",
    "license": "ISC",
    "scripts": {
        "start": "export NODE_OPTIONS=--openssl-legacy-provider && cross-env NODE_ENV=development && node ./config/koa-server.js",
        "build": "cross-env NODE_ENV=production &&  webpack --mode=production --config ./config/webpack.prod.js",
        "prod-server": "npm run build && cross-env NODE_ENV=production node ./config/koa-server.js",
        "just-server": "cross-env NODE_ENV=production node ./config/koa-server.js",
        "build-vendor": "cross-env NODE_ENV=production  webpack --progress --config ./config/webpack.vendor.js",
        "serve": "serve build",
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint:tsx": "eslint --ext .tsx src && eslint --ext .ts src",
        "lint:css": "stylelint --aei .less .css src",
        "precommit": "lint-staged",
        "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
    },
    "pre-commit": [
        "precommit",
        "precommit-msg"
    ],
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ],
        "*.{css,less}": [
            "stylelint --fix",
            "prettier --write",
            "git add"
        ]
    },
    "keywords": [
        "react",
        "webpack",
        "typescript",
        "mobx",
        "hook",
        "react17",
        "webpack5",
        "koa",
        "webpack-dev-middleware",
        "webpack-hot-middleware"
    ],
    "devDependencies": {
        "@babel/cli": "^7.13.10",
        "@babel/core": "^7.13.10",
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/preset-env": "^7.13.10",
        "@babel/preset-react": "^7.12.13",
        "@babel/preset-typescript": "^7.14.5",
        "@babel/runtime": "^7.14.6",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
        "@types/loadable__component": "^5.13.3",
        "@types/react": "^17.0.11",
        "@types/react-dom": "^17.0.7",
        "@types/react-router-config": "^5.0.2",
        "@types/react-router-dom": "^5.1.7",
        "@typescript-eslint/eslint-plugin": "^4.26.1",
        "@typescript-eslint/parser": "^4.26.1",
        "autoprefixer": "^10.2.6",
        "babel-loader": "^8.2.2",
        "babel-plugin-transform-runtime": "^6.23.0",
        "chalk": "^4.1.1",
        "classnames": "^2.3.1",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^5.0.1",
        "css-minimizer-webpack-plugin": "^3.0.1",
        "eslint": "^7.28.0",
        "eslint-import-resolver-webpack": "^0.13.1",
        "eslint-plugin-react": "^7.24.0",
        "eslint-webpack-plugin": "^2.5.4",
        "file-loader": "^6.2.0",
        "friendly-errors-webpack-plugin": "^1.7.0",
        "html-webpack-plugin": "^5.2.0",
        "koa": "^2.13.1",
        "koa-mount": "^4.0.0",
        "koa-static": "^5.0.0",
        "less": "^4.1.1",
        "less-loader": "^10.0.0",
        "lint-staged": "^11.0.0",
        "lodash": "^4.17.20",
        "mini-css-extract-plugin": "^1.6.0",
        "path-browserify": "^1.0.1",
        "postcss": "^8.3.5",
        "postcss-loader": "^6.1.0",
        "prettier": "^2.3.1",
        "react": "^17.0.1",
