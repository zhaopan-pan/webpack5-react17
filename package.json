        "react-refresh": "^0.14.0",
        "react-router-dom": "^5.2.0",
        "style-loader": "^2.0.0",
        "stylelint": "^13.13.1",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-standard": "^22.0.0",
        "typescript": "^4.3.2",
        "url-loader": "^4.1.1",
        "webpack": "5.38.1",
